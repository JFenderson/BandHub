{
  "dashboard": {
    "title": "HBCU Band Hub - Worker Metrics",
    "tags": ["bandhub", "worker", "jobs", "bullmq"],
    "timezone": "browser",
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "id": null,
    "links": [],
    "liveNow": false,
    "panels": [
      {
        "title": "Job Completion Rate",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "sum(rate(bandhub_video_sync_results_total{result=\"success\"}[5m]))",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "sum(rate(bandhub_video_sync_results_total{result=\"failure\"}[5m]))",
            "legendFormat": "Failure",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Job Success Rate %",
        "type": "gauge",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "(sum(rate(bandhub_video_sync_results_total{result=\"success\"}[5m])) / sum(rate(bandhub_video_sync_results_total[5m]))) * 100",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            },
            "min": 0,
            "max": 100
          }
        }
      },
      {
        "title": "Queue Depth by Queue",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "sum(bandhub_queue_depth{state=\"waiting\"}) by (queue)",
            "legendFormat": "{{queue}} - Waiting",
            "refId": "A"
          },
          {
            "expr": "sum(bandhub_queue_depth{state=\"active\"}) by (queue)",
            "legendFormat": "{{queue}} - Active",
            "refId": "B"
          },
          {
            "expr": "sum(bandhub_queue_depth{state=\"delayed\"}) by (queue)",
            "legendFormat": "{{queue}} - Delayed",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "palette-classic" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 100, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "title": "Active Workers",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "sum(bandhub_worker_active_jobs) by (worker_id)",
            "legendFormat": "Worker {{worker_id}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Job Processing Time (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(bandhub_job_processing_seconds_bucket[5m])) by (le))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(bandhub_job_processing_seconds_bucket[5m])) by (le))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(bandhub_job_processing_seconds_bucket[5m])) by (le))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Processing Time by Job Type",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "sum(rate(bandhub_job_processing_seconds_sum[5m])) by (job_type) / sum(rate(bandhub_job_processing_seconds_count[5m])) by (job_type)",
            "legendFormat": "{{job_type}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Job Throughput by Queue",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "sum(rate(bandhub_video_sync_results_total[5m])) by (queue)",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Failed Job Trends",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "sum(rate(bandhub_video_sync_results_total{result=\"failure\"}[5m])) by (queue)",
            "legendFormat": "{{queue}} Failures",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": { "mode": "palette-classic" }
          }
        }
      },
      {
        "title": "Current Queue Depth Alert",
        "type": "stat",
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 32 },
        "targets": [
          {
            "expr": "sum(bandhub_queue_depth{state=\"waiting\"})",
            "legendFormat": "Waiting Jobs",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 100, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "title": "Job Success Rate by Queue",
        "type": "bargauge",
        "gridPos": { "h": 8, "w": 16, "x": 8, "y": 32 },
        "targets": [
          {
            "expr": "(sum(rate(bandhub_video_sync_results_total{result=\"success\"}[5m])) by (queue) / sum(rate(bandhub_video_sync_results_total[5m])) by (queue)) * 100",
            "legendFormat": "{{queue}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            },
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 38,
    "style": "dark",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {},
    "version": 1,
    "weekStart": ""
  }
}
